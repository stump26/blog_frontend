{
  "env": {
    "browser": true,
    "es6": true
  },

  "extends": ["airbnb-base/legacy", "prettier"],
  "rules": {
    "no-use-before-define": ["error", { "functions": false }],
    "no-unused-expressions": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "no-debugger": "warn",
    "no-alert": "off",
    "no-shadow": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "warn"
  },
  "parser": "babel-eslint"
}
